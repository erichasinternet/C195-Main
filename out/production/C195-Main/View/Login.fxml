<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" accessibleRole="PASSWORD_FIELD" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="Controller.loginController">
    <children>
        <TextField fx:id="usernameField" layoutX="226.0" layoutY="188.0"/>
        <PasswordField fx:id="passwordField" accessibleRole="PASSWORD_FIELD" layoutX="226.0" layoutY="223.0">
        </PasswordField>
        <Label fx:id="usernameLabel" layoutX="112.0" layoutY="192.0" text="Username:"/>
        <Label fx:id="passwordLabel" layoutX="114.0" layoutY="227.0" text="Password:"/>
        <Button fx:id="loginButton" layoutX="237.0" layoutY="278.0" mnemonicParsing="false" onAction="#loginButtonClick"
                prefHeight="25.0" prefWidth="126.0" text="Login"/>
        <Label fx:id="locationTitleLabel" alignment="CENTER" layoutX="226.0" layoutY="107.0" text="Location:"
               textAlignment="CENTER"/>
        <Label fx:id="locationLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="274.0" layoutY="103.0"
               prefHeight="25.0" prefWidth="111.0" text="Label" textAlignment="CENTER" textFill="#060d86"/>
        <Label fx:id="loginError" alignment="CENTER" layoutX="184.0" layoutY="369.0" prefHeight="17.0" prefWidth="233.0"
               textAlignment="CENTER" textFill="RED"/>
    </children>
</AnchorPane>
